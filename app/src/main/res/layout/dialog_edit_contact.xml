<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#AAFFFFFF"
    tools:context=".fragments.MovetoIwantFragment">

    <LinearLayout
        android:layout_width="450dp"
        android:layout_height="wrap_content"
        android:orientation="vertical">
        <TextView
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:gravity="center_horizontal"
            android:paddingBottom="10dp"
            android:paddingTop="10dp"
            android:background="#AA444444"
            android:textSize="20sp"
            android:text="@string/input_coord_you_want" />
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="60dp"
            android:background="#AAFFFFFF"
            android:padding="5dp"
            android:orientation="horizontal">
            <TextView
                android:layout_width="75dp"
                android:layout_height="match_parent"
                android:gravity="center_vertical"
                android:layout_marginRight="10dp"
                android:textSize="25sp"
                android:textColor="#FF000000"
                android:text="UTM" />

            <EditText
                android:id="@+id/etUTM1"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:gravity="right|center_vertical"
                android:hint="52S 331158.87E 4120721.44N"
                android:inputType="text"
                android:singleLine="true"
                android:lines="1"
                android:textColor="#FF000000"
                android:textColorHint="#99AAAAAA"
                android:textSize="20sp"
                android:imeOptions="actionSearch"/>
        </LinearLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="60dp"
            android:background="#AAFFFFFF"
            android:padding="5dp"
            android:orientation="horizontal">
            <TextView
                android:layout_width="75dp"
                android:layout_height="match_parent"
                android:gravity="center_vertical"
                android:layout_marginRight="10dp"
                android:textColor="#FF000000"
                android:textSize="25sp"
                android:text="MGRS" />

            <EditText
                android:id="@+id/etMGRS1"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:gravity="right|center_vertical"
                android:hint="52SCG 31159 20721"
                android:inputType="text"
                android:singleLine="true"
                android:lines="1"
                android:textColor="#FF000000"
                android:textColorHint="#99AAAAAA"
                android:textSize="20sp"
                android:imeOptions="actionSearch"/>
        </LinearLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="60dp"
            android:background="#AAFFFFFF"
            android:padding="5dp"
            android:orientation="horizontal">
            <TextView
                android:layout_width="75dp"
                android:layout_height="match_parent"
                android:gravity="center_vertical"
                android:layout_marginRight="10dp"
                android:textColor="#FF000000"
                android:textSize="25sp"
                android:text="GEO" />

            <EditText
                android:id="@+id/etGEOLat"
                android:layout_width="150dp"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:gravity="right|center_vertical"
                android:hint="32.123456"
                android:inputType="numberDecimal"
                android:singleLine="true"
                android:lines="1"
                android:textColor="#FF000000"
                android:textColorHint="#99AAAAAA"
                android:textSize="20sp"
                android:imeOptions="actionSearch" />

            <EditText
                android:id="@+id/etGEOLon"
                android:layout_width="150dp"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:gravity="right|center_vertical"
                android:hint="127.3213232"
                android:inputType="numberDecimal"
                android:singleLine="true"
                android:lines="1"
                android:textColor="#FF000000"
                android:textColorHint="#99AAAAAA"
                android:textSize="20sp"
                android:imeOptions="actionSearch" />
        </LinearLayout>
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="60dp"
            android:background="#AAFFFFFF"
            android:padding="5dp"
            android:orientation="horizontal">
            <TextView
                android:layout_width="75dp"
                android:layout_height="match_parent"
                android:gravity="center_vertical"
                android:layout_marginRight="10dp"
                android:textColor="#FF000000"
                android:textSize="25sp"
                android:text="DM" />

            <EditText
                android:id="@+id/etDMSLat"
                android:layout_width="150dp"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:gravity="right|center_vertical"
                android:hint="32° 12' 33.3456''"
                android:inputType="text"
                android:singleLine="true"
                android:lines="1"
                android:textColor="#FF000000"
                android:textColorHint="#99AAAAAA"
                android:textSize="18sp"
                android:imeOptions="actionSearch" />

            <EditText
                android:id="@+id/etDMSLon"
                android:layout_width="150dp"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:gravity="right|center_vertical"
                android:hint="127° 32' 21.1112''"
                android:inputType="text"
                android:singleLine="true"
                android:lines="1"
                android:textColor="#FF000000"
                android:textColorHint="#99AAAAAA"
                android:textSize="18sp"
                android:imeOptions="actionSearch" />
        </LinearLayout>
        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="50dp"
            android:padding="5dp">

            <Button
                android:id="@+id/btnmylocation"
                android:layout_width="100dp"
                android:layout_height="match_parent"
                android:layout_alignParentLeft="true"
                android:text="@string/my_location"
                android:textSize="15sp" />

            <Button
                android:id="@+id/btn_go"
                android:layout_width="100dp"
                android:layout_height="match_parent"
                android:layout_alignParentRight="true"
                android:text="@string/my_location_go"
                android:textSize="15sp" />

        </RelativeLayout>
    </LinearLayout>

</FrameLayout>



